    "lodash": "^4.17.20"
  },
  "peerDependencies": {
    "mongoose": ">=4"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.7.1",
    "jest": "^26.5.3",
